{
    "bracketSameLine": true,
    "printWidth": 100,
    "semi": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "endOfLine": "auto",
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": { "parser": "svelte" }
        }
    ],
    "svelteStrictMode": true
}
